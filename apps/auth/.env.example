# app
PORT=5002
NODE_ENV="development"

# database
DATABASE_URL="mongodb://host.docker.internal:27017/nestjs-microservices-auth?replicaSet=replicaSet1&directConnection=true"

# rabbitmq
RABBITMQ_URL="amqp://guest:guest@microservices-rabbitmq:5672"
RABBITMQ_AUTH_CLIENT_QUEUE="auth_client_queue"
RABBITMQ_USER_CLIENT_QUEUE="user_client_queue"
RABBITMQ_NOTIFICATION_CLIENT_QUEUE="notification_client_queue"
